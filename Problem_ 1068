#include <stdio.h>

int main(void) {
    char s[1200];

    while (fgets(s, sizeof(s), stdin) != NULL) {
        int bal = 0, ok = 1;

        for (char *p = s; *p; ++p) {
            if (*p == '(') {
                ++bal;
            } else if (*p == ')') {
                --bal;
                if (bal < 0) {      // fechou sem ter aberto
                    ok = 0;
                    break;
                }
            }
        }
        if (bal != 0) ok = 0;       // sobrou '(' sem fechar
        puts(ok ? "correct" : "incorrect");
    }
    return 0;
}
